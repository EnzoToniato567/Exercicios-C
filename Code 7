//biblioteca

#include <stdio.h>
#include <locale.h>


int main() {
    setlocale(LC_ALL, ""); 

    int num, i;

    printf("Digite um número:\n"); //Digite um número para ver sua tabuada
    scanf("%d", &num);

    printf("Tabuada do número %d:\n", num); //Tabuada em laço do for
    for (i = 1; i <= 10; i++) {
        printf("%d x %d = %d\n", num, i, num * i); //Multiplicação dentro do scanf
    }                                  //Num * i = número * a contagem até 10 do for

    return 0;
}