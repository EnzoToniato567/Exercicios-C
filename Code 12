//biblioteca 

#include <stdio.h>
#include <locale.h>

int main() {
    setlocale(LC_ALL, "");

    int num[10], maior, menor, i; //Estrutura array
    
    //For para contar tudo de uma vez
    printf ("Insira 10 números:\n");
    for (i = 0; i < 10; i++) {
    scanf ("%d", &num[i]);
    
    }
    
    //Maior e menor com o primeiro número array
    maior = menor = num[i];
    
    //Utilizando if para  condição verdadeira
    for (i = 1; i < 10; i++) {
        if (num[i] > 10) {
            maior = num[i];
        }
        if (num[i] < 10) {
            menor = num[i]; }
    }
    
    printf("O maior número é: %d\n", maior);
    printf("O menor número é: %d\n", menor);
    
    return 0;
    
}