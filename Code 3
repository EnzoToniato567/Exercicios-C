//biblioteca

#include <stdio.h>
#include <locale.h>

int main() {
    setlocale(LC_ALL, ""); 

    int nota[2], i;
    
    //For como estrutura para ler as 2 notas de uma vez
    for (i = 0; i < 2; i++) {
        printf ("Insira a nota %d:\n", i + 1);
        scanf ("%d", &nota[i]);
    }
    
    //If else para as condições de abaixo da média e acima da média
    if (nota[i] >= 7) {
        printf ("Aprovado!\n");
    }
    
    else if (nota[i] < 7) {
        printf ("Reprovado!\n");
    }
    
    return 0;
    
}