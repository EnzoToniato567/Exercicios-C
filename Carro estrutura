//biblioteca

#include <stdio.h>
#include <locale.h>

typedef struct {
        char modelo[50];
        int ano;
        float preço;
} Carro;

int main () {
        
        Carro carro1 = {"Fusca", 1974, 10000.00};
        
        printf ("Modelo: %s\n", carro1.modelo);
        printf ("Ano: %d\n", carro1.ano);
        printf ("Preço: %.2f", carro1.preço);
        
        return 0;
}